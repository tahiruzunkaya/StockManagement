
@{
  ViewData["Title"] = "Index";
}


  <div class="col-lg-4">
    <div class="card-chart card">
      <div class="card-header">
        <h5 class="card-category">Zimmetteki Ürün </h5>
        <h3 class="card-title">
          Sayısı
        </h3>
      </div>
      <div class="card-body">
        <div class="chart-area">
          <img src="static/img/zimmet.png" alt="">
          <div class="chart-area" style="font-size: 60px">
            @ViewBag.ZimmetSayisi
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-lg-4">
    <div class="card-chart card">
      <div class="card-header">
        <h5 class="card-category">Atıktaki Ürün </h5>
        <h3 class="card-title">
          Sayısı
        </h3>
      </div>
      <div class="card-body">
        <div class="chart-area">
          <img src="static/img/atik.png" alt="">
          <div class="chart-area" style="font-size: 60px">
            @ViewBag.AtikSayisi
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-lg-4">
    <div class="card-chart card">
      <div class="card-header">
        <h5 class="card-category">Çalışan </h5>
        <h3 class="card-title">
          Sayısı
        </h3>
      </div>
      <div class="card-body">
        <div class="chart-area">
          <img src="static/img/calisan.png" alt="">
          <div class="chart-area" style="font-size: 60px">
            @ViewBag.CalisanSayisi
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-md-12">
    <div class="card-chart card">
      <div id="t" class="offline">
        <div id="messageBox" class="sendmessage">
          <h1 style="text-align: center;font-family: 'Open Sans', sans-serif;">
            Press
            Space to start
          </h1>
          <div class="niokbutton" onclick="okbuttonsend()"></div>
        </div>
        <div id="main-frame-error" class="interstitial-wrapper">
          <div id="main-content">
            <div class="icon icon-offline" alt=""></div>
          </div>
          <div id="offline-resources">
            <img id="offline-resources-1x" src="~/vendor/static/dino-assets/default_100_percent/100-offline-sprite.png">
            <img id="offline-resources-2x" src="~/vendor/static/dino-assets//default_200_percent/200-offline-sprite.png">

          </div>
        </div>
      </div>
      <script>
  document.onkeydown = function (evt) {
          evt = evt || window.event;
          if (evt.keyCode == 32) {
            var box = document.getElementById("messageBox");
            box.style.visibility = "hidden";
          }
        };</script>
    </div>
  </div>





